function Math() {
	linear_equation = function(_number){
		return 5 * _number
	}
	
	square_root_equation = function(_number){
		return sqrt(_number)
	}
	
	square_root_equation_pow = function(_number){
		return power(_number, 0.5)
	}
	
	square_root_equation_pow = function(_number){
		return power(_number, 0.5)
	}
	
	polynominal_equation = function(_number){
		return power(_number, 2);
	}
	
	exponencial_equation = function(_number){
		return power(2,_number)
	}
	
	log_equation = function(_number){
		return logn(_number, 2)
	}
	
	log_equation_2 = function(_number){
		return power(10, _number)
	}
	
	exponential_equation_2 = function(_number){
		return power(_number, 0.5)
	}
}

function Exponencial_Equation (_number){
		return power(2,_number)
	}
	
	
	
